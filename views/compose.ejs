<%- include('partials/header') -%>

<h1>Compose</h1>


<form action='/compose' method='post'>
  <div class="form-group">
    <label>Title</label>
    <input name="newTitle" class="form-control" rows="1">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea name="newPost" class="form-control" rows="5" cols="30"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>

</form>








<%- include('partials/footer') -%>